{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-js/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-js/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-js",
  "projects": {
    "mongoose": {
      "type": "library",
      "root": "libs/mongoose",
      "entryFile": "index",
      "sourceRoot": "libs/mongoose/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mongoose/tsconfig.lib.json"
      }
    },
    "ms-binance": {
      "type": "application",
      "root": "apps/ms-binance",
      "entryFile": "main",
      "sourceRoot": "apps/ms-binance/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ms-binance/tsconfig.app.json"
      }
    },
    "ms-order": {
      "type": "application",
      "root": "apps/ms-order",
      "entryFile": "main",
      "sourceRoot": "apps/ms-order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ms-order/tsconfig.app.json"
      }
    },
    "ms-orders": {
      "type": "application",
      "root": "apps/ms-orders",
      "entryFile": "main",
      "sourceRoot": "apps/ms-orders/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ms-orders/tsconfig.app.json"
      }
    },
    "ms-quant": {
      "type": "application",
      "root": "apps/ms-quant",
      "entryFile": "main",
      "sourceRoot": "apps/ms-quant/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ms-quant/tsconfig.app.json"
      }
    },
    "nest-js": {
      "type": "application",
      "root": "apps/nest-js",
      "entryFile": "main",
      "sourceRoot": "apps/nest-js/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-js/tsconfig.app.json"
      }
    }
  }
}